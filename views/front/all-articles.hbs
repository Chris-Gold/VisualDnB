      <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav d-flex justify-content-around">
          <li class="nav-item">
            <button id="eng" type="button" class="fr"><img class="img-fluid" src="img/uk.gif"/></button>
            <button id="fr" type="button" class="eng d-none "><img class="img-fluid" src="img/fr2.gif"/></button>
          </li>
          <li class="nav-item btn-2 ">
            <a class="nav-link " href="#header"><span class="fr">Accueil</span><span class="eng d-none">Home</span></a>
          </li>
          <li class="nav-item btn-2">
            <a class="nav-link" href="#about"><span class="fr">Qui nous sommes</span><span class="eng d-none">Who we are</span></a>
          </li>
          <li class="nav-item btn-2">
            <a class="nav-link" href="#presta"><span class="fr">Nos Prestations</span><span class="eng d-none">Our Performances</span></a>
          </li>
          <li class="nav-item btn-2">
            <a class="nav-link" href="#team"><span class="fr">L'Ã©quipe</span><span class="eng d-none">The Team</span></a>
          </li>
        </ul>
      </div>
    </nav>

    <section class="col-12 text-center">
        <div>{{title}}</div>
        <div>{{description}}</div>
    </section>
    <section class="col-12 text-center">
        {{#each blogPosts}}
            Titre : {{this.title}}</br>
            message : {{this.message}}</br>
            <div>
                <a href="/edit-article/{{this._id}}">modifier</a> &nbsp;
                <a href="/delete-article/{{this._id}}">supprimer</a>
            </div>
            <hr>
        {{/each}}
    </section>
    
    <footer class="box text-center d-flex align-items-center">
      <div class="w-100">
        <p>Ceci est un footer...oui oui!!</p>
      </div>
    </footer>

    <!-- INSERTION SCRIPT -->

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js" integrity="sha384-cs/chFZiN24E4KMATLdqdvsezGxaGsi4hLGOzlXwp5UZB1LY//20VyM2taTB4QvJ" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js" integrity="sha384-uefMccjFJAIv6A+rW+L4AHf99KvxDjWSu1z9VI8SKNVmz4sk7buKt/6v9KI65qnm" crossorigin="anonymous"></script>
    <script type="text/javascript" src="js/slider/jquery.cslider.js"></script>
