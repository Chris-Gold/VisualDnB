<header id="header" class="container-fluid text-center">
  <!-- VIDEO PRESENTATION -->
  {{#each visuels}}
  <iframe id="topVid" src="http://player.vimeo.com/video/{{this.video}}?muted=1&loop=1&autoplay=1" frameborder="0">
  </iframe>
  <div id="vimButton" class="row">
    <div id="pause" class="col-6 text-left">
      <button id="vimPlay" type="button" style="display: none"><i class="fas fa-play fa-lg"></i></button>
      <button id="vimPause" type="button"><i class="fas fa-pause fa-lg"></i></button>
    </div>
    <div id="mute" class="col-6 text-right">
      <button id="vimMute" type="button" style="display: none"><i class="fas fa-volume-off fa-lg"></i></button>
      <button id="vimUnmute" type="button"><i class="fas fa-volume-up fa-lg"></i></button>
    </div>
  </div>

  {{/each}}

</header>

