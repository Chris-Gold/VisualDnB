    <section id="contact" class="container-fluid box text-center">
      <h3><span class="fr">Contactez nous!!</span><span class="eng d-none">Contact us!!</span></h3>
      <div class="container">
        <div class="row">
        <!-- CONTACT + CALENDRIER -->
          <form id="contactForm" action="/" class="col-12 col-md-6 box" method="POST">
            <input type="text" class="col-12" name="nom" placeholder="Nom" required>
            <input type="email" class="col-12" name="email" placeholder="Email" required>
            <input type="button" class="inputButton" value="Logo" name="logo">
            <input type="button" class="inputButton" value="ClipAudioReact" name="clipAudioReact">
            <input type="button" class="inputButton" value="Bumper" name="bumper">
            <input type="button" class="inputButton" value="ClipVJ" name="clipVJ">
            <input type="button" class="inputButton" value="ClipVideo" name="clipVideo">
            <input type="button" class="inputButton" value="Scenographie" name="scenographie">
            <input type="button" class="inputButton" value="Autre" name="autre">
            <p>* Les boutons servent à remplir le sujet du mail.</p>
            <input type="text" class="col-12" id="subject" name="subject" placeholder="Sujet" required>
            <textarea maxlength="1000" type="text" class="col-12" name="message" placeholder="Message" required></textarea>
            <input type="submit" class="col-12" value="Envoyer"></input>
          </form>

          <div id="calendrier" class="col-12 col-md-6 box d-flex text-center">
            <iframe src="https://calendar.google.com/calendar/embed?src=chris.ratsimba87%40gmail.com&ctz=Europe%2FParis"></iframe>
          </div>
        </div>
      </div>
    </section>

