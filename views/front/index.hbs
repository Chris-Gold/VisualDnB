          <div class="da-slide row">
            <div class="col-12 col-md-6">
              <h4>{{this.titre}}</h4>
              <p>{{this.description}}</p>
              <a href="#" class="da-link">Read more</a>
            </div>
            <div class="da-img col-12 col-md-6 d-flex justify-content-center"><img class="img-fluid" src="https://visualdnb.files.wordpress.com/2018/04/10ansbigroom-012-e1523480499310.jpg?w=1024&h=515&crop=1" alt="image01" /></div>
          </div>
          {{/each}}

          <nav class="da-arrows">
            <span class="da-arrows-prev"></span>
            <span class="da-arrows-next"></span>
          </nav>
        </div>
      </div>
    </section>

