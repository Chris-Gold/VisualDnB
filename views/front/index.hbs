<header class="container-fluid text-center">
  <!-- VIDEO PRESENTATION -->
  {{#each visuels}}

  <iframe id="topVid" src="https://player.vimeo.com/video/{{this.video}}?loop=1&muted=1&autoplay=1" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen>
  </iframe>
  <div id="vimButton" class="row">
    <div id="pause" class="col-6 text-left">
      <button id="vimPlay" type="button" style="display: none">Play</button>
      <button id="vimPause" type="button">Pause</button>
    </div>
    <div id="mute" class="col-6 text-right">
      <button id="vimMute" type="button" style="display: none">Mute</button>
      <button id="vimUnmute" type="button">Unmute</button>
    </div>
  </div>  

  {{/each}}

</header>

