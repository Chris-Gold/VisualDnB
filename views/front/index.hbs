        <input type="button" class="inputButton btn btn-outline-success" value="ClipVideo" name="clipVideo">
        <input type="button" class="inputButton btn btn-outline-success" value="Scenographie" name="scenographie">
        <input type="button" class="inputButton btn btn-outline-success" value="Autre" name="autre">
        <input type="text" class="col-12" id="subject" name="subject" placeholder="Sujet" required>
        <textarea maxlength="1000" type="text" class="col-12" name="message" placeholder="Message" style="border-radius:1em;
        " required></textarea>
        <input type="submit" id="submit" class="col-12" value="Envoyer">
