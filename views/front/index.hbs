<section id="contact" class="container-fluid box text-center">
  <h3><span class="fr">Contactez nous!!</span><span class="eng d-none">Contact us!!</span></h3>
  <div class="container">
    <div class="row">
    <!-- CONTACT + CALENDRIER -->
      <form id="contactForm" action="/" class="col-12 col-md-6 box" method="POST">
        <input type="text" class="col-12" name="nom" placeholder="Nom" required>
        <input type="email" class="col-12" name="email" placeholder="Email" required>
        <p class="toGreen"><span class="fr">Vous êtes intéressé par :</span><span class="eng d-none">You are interesting in :</span></p>
        <input type="button" class="inputButton btn btn-outline-success" value="Logo" name="logo">
