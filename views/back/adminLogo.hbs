<section>
  <form action="/admin/logo" method="POST">
  
    <div class="row text-center">
        <div class="col-6" style="margin-top: 80px;">
            <p>{{description}}</p>
            <input type="text" name="url" placeholder="image url" value=""><hr>
            <input type="text" name="nom" placeholder="nom" value=""><hr>
            <input type="submit" value="Envoyer">
        </div>
        <div class="col-6">
            <section id="booked">
              <div id="bookedLogos" class="container">
                {{#each logos}}
                  <img class="img-fluid bubble2 animated rollIn" src="../img/{{this.url}}" title="{{this.nom}}"/>
                  <a href="/admin/delete-logo/{{this._id}}">Supprimer</a>
                {{/each}}
              </div>
            </section>
        </div>
      </div>

    </form>
</section>
